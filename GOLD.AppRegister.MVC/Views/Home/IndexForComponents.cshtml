@model IEnumerable<GOLD.AppRegister.ApiModels.ComponentByInterfaceFullName>

@{
    ViewBag.Title = "IndexForComponents";
}

<h2>IndexForComponents</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
