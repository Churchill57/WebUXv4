        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsSecondaryApp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrimaryAppRoute)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondaryAppRoute)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DomainID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DomainName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
